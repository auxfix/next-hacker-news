// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model News {
  id        String     @default(cuid()) @id
  authorId     String
  img   String
  karma Int
  num    Int
  score  Int
  time Int
  title String
  url String
}
